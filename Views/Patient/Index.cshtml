@model IEnumerable<Patient>

<h2>Patients</h2>
<a href="@Url.Action("Create")">Add New Patient</a>
<table>
<thead>
<tr>
<th>Name</th>
<th>Address</th>
<th>Medical Records</th>
</tr>
</thead>
<tbody>
@foreach (var patient in Model)
{
<tr>
<td>@patient.Name</td>
<td>@patient.Address</td>
<td>@patient.MedicalRecords</td>
</tr>
}
</tbody>
</table>
